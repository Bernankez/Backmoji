{
  "name": "@backmoji/react",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.15.5",
  "description": "React port for backmoji",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com",
    "url": "https://github.com/Bernankez"
  },
  "license": "MIT",
  "homepage": "https://github.com/Bernankez/backmoji/tree/master/packages/react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/backmoji.git",
    "directory": "packages/react"
  },
  "bugs": "https://github.com/Bernankez/backmoji/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "tsx src/index.ts",
    "watch": "tsx watch src/index.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "backmoji": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "react": "16.8.0",
    "react-dom": "16.8.0"
  }
}
