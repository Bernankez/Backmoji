{
  "name": "@backmoji/vue",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@9.0.2",
  "description": "Vue port for backmoji",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com",
    "url": "https://github.com/Bernankez"
  },
  "license": "MIT",
  "homepage": "https://github.com/Bernankez/backmoji/tree/master/packages/vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/backmoji.git",
    "directory": "packages/vue"
  },
  "bugs": "https://github.com/Bernankez/backmoji/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "tsx src/index.ts",
    "watch": "tsx watch src/index.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "backmoji": "workspace:*",
    "vue-demi": "^0.14.7"
  },
  "devDependencies": {
    "vue": "^3.4.23"
  }
}
